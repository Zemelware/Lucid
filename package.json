{
  "name": "lucid",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20 <23"
  },
  "scripts": {
    "dev": "npx -y node@22 ./node_modules/next/dist/bin/next dev",
    "clean": "rm -rf .next .next-build",
    "build": "npm run clean && npx -y node@22 ./node_modules/next/dist/bin/next build",
    "start": "npx -y node@22 ./node_modules/next/dist/bin/next start",
    "lint": "next lint",
    "typecheck": "npx -y node@22 ./node_modules/next/dist/bin/next typegen && tsc --noEmit --incremental false"
  },
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.34.0",
    "@openrouter/sdk": "latest",
    "framer-motion": "latest",
    "lucide-react": "latest",
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zustand": "latest"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.0",
    "glob-parent": "^6.0.2",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.0.0"
  }
}
